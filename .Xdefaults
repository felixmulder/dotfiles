#include "/home/fixel/.colors/smlb"

URxvt.scrollstyle: plain
URxvt.scrollBar: off
!URxvt*font: -misc-fixed-medium-r-semicondensed-*-14-90-100-*-c-60-iso8859-1
URxvt*font: -*-terminus-medium-*-*-*-14-*-*-*-*-*-*-*
!URxvt*font: xft:Meslo LG M-8

URxvt*termName:rxvt-unicode-256color
URxvt.jumpScroll: false

URxvt.letterSpace: 0
URxvt.internalBorder: 2
URxvt*loginShell: true
Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: false
Xft.hinstyle: hintfull
Xft.lcdfilter: lcddefault
URxvt*xftAntialias: true

*urgentOnBell: true

urxvt.perl-ext-common: default,matcher,clipboard,url-select,keyboard-select
URxvt.url-launcher: /usr/bin/firefox
URxvt.iso14755: false
URxvt.matcher.button: 1
URxvt.colorUL: #2960C7

URxvt.scrollTtyOutput: false
URxvt.scrollWithBuffer: true
URxvt.scrollTtyKeypress: true
urxvt*secondaryScroll: false
urxvt*saveLines: 500
URxvt*keysym.Home: \033[1~
URxvt*keysym.End: \033[4~
URxvt.keysym.Shift-Control-C : perl:clipboard:copy
URxvt.keysym.Shift-Control-V : perl:clipboard:paste
URxvt.url-select.launcher: firefox
URxvt.url-select.underline: true
